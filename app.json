    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "02ac09c9-b7d9-47ae-bdd1-642d90666abd"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/02ac09c9-b7d9-47ae-bdd1-642d90666abd"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Medsing to access your camera."
        }
      ]
    ]
  }
